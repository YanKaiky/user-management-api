generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Peoples {
  guid       String   @id @unique() @default(uuid()) @db.Uuid
  name       String
  last_name  String?
  email      String   @unique
  cpf        String
  birth_date DateTime
  createdAt  DateTime @default(now())
  updatedAt  DateTime @default(now()) @updatedAt

  @@map("peoples")
}
